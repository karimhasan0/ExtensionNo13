{
  "manifest_version": 3,
  "name": "Dropship Product Importer",
  "version": "0.1",
  "description": "Import products from suppliers into Shopify",
  "host_permissions": [
    "https://dropbridge.onrender.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.aliexpress.com/*",
        "https://www.aliexpress.us/*",
        "https://aliexpress.us/*",
        "https://www.aliexpress.ru/*",
        "https://www.aliexpress.fr/*",
        "https://www.aliexpress.es/*",
        "https://www.aliexpress.de/*",
        "https://www.aliexpress.it/*",
        "https://www.aliexpress.co.uk/*"



        
      ],
      "js": ["content.js"]
    }
  ]
}
